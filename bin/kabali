#!/usr/bin/env ruby
require 'bundler/setup'
require 'kabali'

code1 = <<END
puts 1+1
END

code2 = <<END
10.times do
  puts 1+1
end 
END

code3 = <<END
  10.times do 
    10.times do
      puts 1+1
    end
  end
END

code4 = <<END
 10.times do 
   puts "abcd"
 end
END

code5 =<<CODE
def display_string
  str = "Dynamic access."
  10.times do
    10.times do
      str = str + "!"
      puts str
    end 
  end
end
puts display_string()
CODE

k = Kabali::Kabali.new(code5)   # Change the value here
k.traverse
